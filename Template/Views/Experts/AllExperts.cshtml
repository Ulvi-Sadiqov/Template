@model List<Template.Data.Expert>
<div class="row">

	@foreach (var expert in Model)
	{
		<div class="col-lg-3 col-sm-6">
			<div class="team__item">
				<div class="team__item__pic">
					<img src="~/Content/images/about/@expert.Image" alt="">
				</div>
				<div class="team__item__text">
					<h5>@expert.Username</h5>
					<span>@expert.Job</span>
				</div>
			</div>
		</div>
	}

</div>

